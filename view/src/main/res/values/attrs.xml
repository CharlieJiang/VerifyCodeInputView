<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  自定义验证码输入框相关属性  -->
    <declare-styleable name="VerifyCodeInputViewStyle">
        <attr name="boxWidth" format="dimension"/>
        <attr name="boxHeight" format="dimension"/>
        <attr name="boxSpacing" format="dimension"/>
        <attr name="boxBorderHeight" format="dimension"/>
        <attr name="boxCount" format="integer"/>
        <attr name="boxBorderColorNormal" format="color"/>
        <attr name="boxBorderColorFocused" format="color"/>
        <attr name="autoFit" format="boolean"/>
    </declare-styleable>
</resources>